{
    "builds": [
      {
        "src": "degreatarizona/wsgi.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb", 
          "runtime": "python3.9" 
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "degreatarizona/wsgi.py"
      }
    ],
    "rewrites": [
    { 
      "source": "/static/(.*)", 
      "destination": "/static/$1" 
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
    "buildCommand": "pip install -r requirements.txt"
  }
  