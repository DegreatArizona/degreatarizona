{
    "builds": [
      {
        "src": "degreatarizona/wsgi.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb", 
          "runtime": "python3.9" 
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "degreatarizona/wsgi.py"
      }
    ],
    "rewrites": [
    { 
      "source": "/static/(.*)", 
      "destination": "/static/$1" 
    }
  ],
    "buildCommand": "pip install -r requirements.txt"
  }
  